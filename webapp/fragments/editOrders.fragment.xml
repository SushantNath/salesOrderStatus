<core:FragmentDefinition xmlns:m="sap.m" xmlns="sap.m" xmlns:core="sap.ui.core" displayBlock="true" height="100%">
	<m:ScrollContainer horizontal="true" vertical="true" height="100%" class="sapUiTinyMarginEnd">
		<m:Table id="selfOrdersTable" class="sapUiResponsiveMargin sapUiNoMarginTop verticalBorder" fixedLayout="auto" mode="MultiSelect"
			items="{path:'allOrdersModel>/allOrdersSet', sorter: {path: 'CreatedDate', descending: true}}" growing="true" growingScrollToLoad="false"
			growingThreshold="100" width="95%" updateFinished="onUpdateFinished" selectionChange="onPressOrderNumber"
			sticky="ColumnHeaders,HeaderToolbar">
			<m:headerToolbar></m:headerToolbar>
			<columns>
				<Column id="orderCol" visible="true"><Label wrapping="true" design="Bold" text="{i18n>orderCol}" width="60px"/></Column>
				<Column id="orderStatusCol" minScreenWidth="Tablet" demandPopin="true" visible="true"><Label wrapping="true" design="Bold" text="{i18n>orderStatusCol}" width="80px"/></Column>
				<Column id="POCol" minScreenWidth="Tablet" demandPopin="true" visible="true"><Label wrapping="true" design="Bold" text="{i18n>POCol}" width="70px"/></Column>
				<Column id="CreateDateCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="true"><Label wrapping="true" design="Bold" text="{i18n>CreateDateCol}" width="80px"/></Column>
				<Column id="CreateTimeCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="true"><Label wrapping="true" design="Bold" text="{i18n>CreateTimeCol}" width="80px"/></Column>
				<Column id="DeliveryIdCol" hAlign="Center" visible="true"><Label wrapping="true" design="Bold" text="{i18n>DeliveryIdCol}" width="90px"/></Column>
				<Column id="shipToCol"><Label wrapping="true" design="Bold" text="{i18n>shipToCol}" width="auto" visible="true"/></Column>
				<Column id="deliveryLocationCol" minScreenWidth="Tablet" demandPopin="true" visible="false"><Label wrapping="true" design="Bold" text="{i18n>deliveryLocationCol}" width="80px"/></Column>
				<Column id="deliveryNumberCol" minScreenWidth="Tablet" demandPopin="true" visible="false"><Label wrapping="true" design="Bold" text="{i18n>deliveryNumberCol}" width="80px"/></Column>
				<Column id="duplicateCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="true"><Label wrapping="true" design="Bold" text="{i18n>duplicateCol}" width="80px"/></Column>
				<Column id="amendCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="true"><Label wrapping="true" design="Bold" text="{i18n>amendCol}" width="80px"/></Column>
				<Column id="requestedDeliveryCol" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>requestedDeliveryCol}" width="80px"/></Column>
				<Column id="actualDeliveryCol" visible="false"><Label wrapping="true" design="Bold" text="{i18n>actualDeliveryCol}" width="80px"/></Column>
				<Column id="invoiceNumCol" minScreenWidth="Tablet" demandPopin="true" visible="false"><Label wrapping="true" design="Bold" text="{i18n>invoiceNumCol}" width="80px"/></Column>
				<Column id="invoiceDCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>invoiceDCol}" width="80px"/></Column>
				<Column id="invoiceAmountCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>invoiceAmountCol}" width="80px"/></Column>
				<Column id="paymentDueCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>paymentDueCol}" width="80px"/></Column>
				<Column id="paymentDocCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>paymentDocCol}" width="80px"/></Column>
				<Column id="paidCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>paidCol}" width="80px"/></Column>
				<Column id="sapPoNoCol" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" visible="false"><Label wrapping="true" design="Bold" text="{i18n>sapPoCol}" width="80px"/></Column>
			</columns>
			<items>
				<ColumnListItem>
					<customData>
						<core:CustomData key="orderNumber" value="{allOrdersModel>IOrderNumber}"/>
					</customData>
					<customData>
								<core:CustomData key="deliveryNumber" value="{allOrdersModel>DeliveryNumberExt}"/>
					</customData>
					<cells>
						<m:Link press="_onOrderClick" text="{allOrdersModel>OrderNumberExt}" visible="true" textAlign="Begin" width="auto" target="_blank"
							enabled="true" emphasized="false" subtle="false">
							<customData>
								<core:CustomData key="orderNumber" value="{allOrdersModel>IOrderNumber}"/>
							</customData>
						</m:Link>
						<Text text="{allOrdersModel>StatusText}"/>
						<Text text="{allOrdersModel>ICustPoNumber}"/>
						<Text text="{path: 'allOrdersModel>CreatedDate', formatter: '.formatter.formatStringDate'}"/>
						<Text text="{ parts: [ 'allOrdersModel>CreatedTime' ], formatter: '.formatCreatedTime' }"/>
						<Text text="{allOrdersModel>ShipToExt}"/>
						<Text text="{allOrdersModel>ShipToName}"/>
						<Text text="{allOrdersModel>ShipToAddress}"/>
						<!--<Text text="{allOrdersModel>DeliveryNumberExt}">-->
						<!--	<customData>-->
						<!--		<core:CustomData key="deliveryNumber" value="{allOrdersModel>DeliveryNumberExt}"/>-->
						<!--	</customData>-->
						<!--</Text>-->
						<m:Link press="onDeliveryNumberClick" text="{allOrdersModel>DeliveryNumberExt}" visible="true" textAlign="Begin" width="auto" target="_blank"
							enabled="{= ${allOrdersModel>ShipmentNumberExt} !== '' ? true : false}" emphasized="false" subtle="false">
							<customData>
								<core:CustomData key="shipmentNumberExt" value="{allOrdersModel>ShipmentNumberExt}"/>
							</customData>
							<customData>
								<core:CustomData key="shipToExt" value="{allOrdersModel>ShipToExt}"/>
							</customData>
						</m:Link>
						<core:Icon id="duplicateIcon" visible="{= ${allOrdersModel>DuplicateAllowed} === 'X' }" src="sap-icon://copy" color="#031E48"
							tooltip="Duplicate" press="onDuplicateOrder"/>
						<core:Icon id="amendIcon" visible="{= ${allOrdersModel>AmendAllowed} === 'X' }" src="sap-icon://edit" color="#031E48" tooltip="Amend"
							press="onEditOrder"/>
						<Text text="{path: 'allOrdersModel>ReqDelvDate', formatter: '.formatter.formatStringDate'}"/>
						<Text text="{path: 'allOrdersModel>ActDelvDate', formatter: '.formatter.formatStringDate'}"/>
						<!--<Text text="{allOrdersModel>InvoiceNumberExt}"/>-->
						<m:Link press="onInvoiceClick" text="{allOrdersModel>InvoiceNumberExt}" visible="true" textAlign="Begin" width="auto" target="_blank"
							enabled="true" emphasized="false" subtle="false">
							<customData>
								<core:CustomData key="invoiceNumber" value="{allOrdersModel>InvoiceNumberExt}"/>
							</customData>
						</m:Link>
						<Text text="{path: 'allOrdersModel>InvoiceDate', formatter: '.formatter.formatStringDate'}"/>
						<Text text="{allOrdersModel>InvoiceAmount}"/>
						<Text text="{path: 'allOrdersModel>PaymentDueDate', formatter: '.formatter.formatStringDate'}"/>
						<m:Link press="_onEFTClick" text="{allOrdersModel>PaymentDocExt}" visible="true" textAlign="Begin" width="auto" target="_blank"
							enabled="true" emphasized="false" subtle="false">
							<customData>
								<core:CustomData key="eftPaidDate" value="{allOrdersModel>PaymentRunDate}"/>
							</customData>
						</m:Link>
						<Text text="{path: 'allOrdersModel>PaidDate', formatter: '.formatter.formatStringDate'}"/>
						<Text text="{allOrdersModel>PurchOrdNumber}"/>
					</cells>
				</ColumnListItem>
			</items>
		</m:Table>
	</m:ScrollContainer>
</core:FragmentDefinition>